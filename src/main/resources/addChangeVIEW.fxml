         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="fromHourLabel" layoutX="215.0" layoutY="99.0" text="From Hour" />
      <Label fx:id="toHourLabel" layoutX="311.0" layoutY="99.0" text="To Hour" />
      <Label fx:id="statusLabel" layoutX="83.0" layoutY="216.0" prefHeight="34.0" prefWidth="478.0" text="WAIT FOR ADD CHANGE" textFill="#14f004">
         <font>
            <Font name="Arial" size="18.0" />
         </font>
      </Label>
      <Spinner fx:id="fromHourSpinner" layoutX="214.0" layoutY="137.0" prefHeight="26.0" prefWidth="58.0" />
